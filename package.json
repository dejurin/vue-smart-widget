{
  "name": "vue-smart-widget",
  "version": "0.5.0",
  "description": "Smart widget component for Vue2.5+.",
  "author": "xiaoluoboding <xiaoluoboding@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest docs",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --mode lib --target lib --dest lib src/index.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "report": "vue-cli-service build --mode lib --target lib --dest lib --report src/index.js"
  },
  "main": "lib/vue-smart-widget.common.js",
  "unpkg": "lib/vue-smart-widget.umd.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaoluoboding/vue-smart-widget.git"
  },
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.10",
    "core-js": "^3.4.2",
    "vue-grid-layout": "^2.3.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "echarts": "^4.5.0",
    "element-ui": "^2.12.0",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "svg-sprite-loader": "^4.1.6",
    "v-tooltip": "^2.0.2",
    "ve-charts": "^0.8.3",
    "vue-router": "^3.0.3",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.0.1"
  },
  "keywords": [
    "widget",
    "card",
    "panel",
    "vue",
    "grid",
    "drag",
    "draggable",
    "resize",
    "resizable"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
